{% extends 'base.html.twig' %}

{% block title %}{% endblock %}

{% block body %}
<div class="deletion_container">
<h1>Suppression d'une sortie</h1>
    <div class="deletion_content">
    {{ form_start(form) }}
    {{ form_row(form.nom) }}
    {{ form_row(form.dateHeureDebut) }}
    {{ form_row(form.site) }}
    <div>
        {{ form_row(form.lieu) }}
    </div>
    {{ form_row(form.annulationMotif) }}
    {{ form_end(form) }}
    </div>
</div>
{% endblock %}
